<script>
$(function() {    
    $('#entitled_discount_detail').dataTable();
});
</script>


<h2>Entitled Discount Details <a href="/entitle_discount_details/new?ref_type_id=<%=params[:id]%>"><button type="button" class="btn btn-link" style="float:right">Add Reference Detail</button></a></h2>

<table id="entitled_discount_detail" class="table table-striped table-bordered">
 <thead>   
  <tr>
    <th>Name</th>
    <th>Address</th>
    <th>Contact no</th>
    <th>Tax pan</th>
    <th>Specialize</th>
    <th>Contact person</th>
    <th>Dept</th>
    <th>Designation</th>
    <th>Rate</th>
    <th>Ref code</th>
    <th>Actions</th>
    
  </tr>
</thead>
<tbody>
<%if @entitle_discount_details.present?%>
<% @entitle_discount_details.each do |entitle_discount_detail| %>
  <tr>
    <td><%= entitle_discount_detail.name %></td>
    <td><%= entitle_discount_detail.address %></td>
    <td><%= entitle_discount_detail.contact_no %></td>
    <td><%= entitle_discount_detail.tax_pan %></td>
    <td><%= entitle_discount_detail.specialize %></td>
    <td><%= entitle_discount_detail.contact_person %></td>
    <td><%= entitle_discount_detail.dept %></td>
    <td><%= entitle_discount_detail.designation %></td>
    <td><%= entitle_discount_detail.rate %></td>
    <td><%= entitle_discount_detail.ref_code %></td>
    
    <td><%#= link_to 'Show', entitle_incentive_detail %><%= link_to 'Edit', edit_entitle_discount_detail_path(entitle_discount_detail) %><%= link_to 'Destroy', entitle_discount_detail, method: :delete, data: { confirm: 'Are you sure?' } %></td>
   
   
  </tr>
<% end %>
<%else%>
 <tr>
<td colspan="11" style="text-align:center">No Entitled Discount Details Found!</td>
</tr>
<%end%>
</tbody>
</table>
