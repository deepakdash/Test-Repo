<script>
$(function() {    
    $('#entitled_commision').dataTable();
});
</script>

<h2>Entitled Incentives/Commision Type</h2>

<table id="entitled_commision" class="table table-striped table-bordered">

 <thead>
    <tr>      
      <th> Reference Type </th>
     <th> Reference Code </th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
<% @entitle_incentives.each do |entitle_incentive| %>
  <tr>
    <td><%= link_to entitle_incentive.ref_type,"/entitles_incentive_detail/#{entitle_incentive.id}" %></td>
      <td><%= entitle_incentive.ref_code %></td>
    <td><%= link_to 'Edit', edit_entitle_incentive_path(entitle_incentive) %><%= link_to 'Destroy', entitle_incentive, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</tbody>
</table>


<h2>Entitled Discounts Type</h2>

<table id="entitled_dis" class="table table-striped table-bordered">

 <thead>
    <tr>      
      <th> Reference Type </th>
     <th> Reference Code </th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
<% @entitle_discounts.each do |entitle_discount| %>
  <tr>
    <td><%= link_to entitle_discount.ref_type,"/entitles_discount_detail/#{entitle_discount.id}" %></td>
    <td><%= entitle_discount.ref_code %></td>
    
    <td><%= link_to 'Edit', edit_entitle_discount_path(entitle_discount) %><%= link_to 'Destroy', entitle_discount, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    
  </tr>
<% end %>
</tbody>
</table>
